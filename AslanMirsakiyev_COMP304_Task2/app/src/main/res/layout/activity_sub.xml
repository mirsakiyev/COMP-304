<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".SubActivity">


    <TextView
        android:id="@+id/nameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name TextView"/>

    <TextView
        android:id="@+id/qualificationTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Qualification TextView"/>

    <TextView
        android:id="@+id/dreamjobTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dream Job TextView"/>

    <TextView
        android:id="@+id/specfieldTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Specialized Field TextView"/>

</LinearLayout>